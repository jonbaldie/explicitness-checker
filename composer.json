  "support": {
    "issues": "https://github.com/jonbaldie/explicitness-checker/issues",
    "source": "https://github.com/jonbaldie/explicitness-checker"
  },
  "authors": [
    {
      "name": "Jonathan Baldie",
      "email": "jon@jonbaldie.com"
    }
  ],
  "require": {
    "php": ">=8.0",
    "nikic/php-parser": "^5.0"
  },
  "bin": [
    "bin/explicitness-checker"
